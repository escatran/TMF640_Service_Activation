            "type": "object", 
            "properties": {
                "body": {
                    "type": "string"
                }, 
                "header": {
                    "type": "array", 
                    "items": {
                        "$ref": "HeaderItem.schema.json#/definitions/HeaderItem"
                    }, 
                    "title": "header"
                }, 
                "statusCode": {
